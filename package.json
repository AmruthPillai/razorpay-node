{
  "name": "rzp-node",
  "version": "0.1.0",
  "description": "Razorpay's bindings for nodejs",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build-server": "babel lib -d dist",
    "build": "npm run clean && npm run build-server",
    "debug": "npm run build && node-debug examples/index.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:razorpay/rzp-node.git"
  },
  "keywords": [
    "razorpay",
    "node",
    "nodejs",
    "rzp-node",
    "rzp"
  ],
  "author": "Abhay Rana",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^1.10.0",
    "mocha": "^2.0.1"
  },
  "dependencies": {
    "request": "^2.75.0",
    "request-promise": "^4.1.1"
  }
}
